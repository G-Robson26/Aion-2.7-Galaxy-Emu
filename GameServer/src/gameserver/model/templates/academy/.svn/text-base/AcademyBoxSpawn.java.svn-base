package gameserver.model.templates.academy;import javax.xml.bind.annotation.XmlAccessType;import javax.xml.bind.annotation.XmlAccessorType;import javax.xml.bind.annotation.XmlAttribute;import javax.xml.bind.annotation.XmlTransient;import javax.xml.bind.annotation.XmlType;import gameserver.model.academy.AcademyBox;@XmlAccessorType(value=javax.xml.bind.annotation.XmlAccessType.FIELD)@XmlType(name="box_loc")public class AcademyBoxSpawn{    public AcademyBoxSpawn() {    }    public float getX(){        return x;    }    public float getY() {        return y;    }    public float getZ() {        return z;    }    public byte getHeading() {        return heading;    }    public AcademyBox getSpawnGroup(){        return academySpawnGroup;    }    public void setSpawnGroup(AcademyBox academybox) {        academySpawnGroup = academybox;    }    public int getWorldId(){        return academySpawnGroup.getMapId();    }    public int getNpcId() {        return academySpawnGroup.getNpcId();    }    @XmlTransient    private AcademyBox academySpawnGroup;    @XmlAttribute(name="x")    private float x;    @XmlAttribute(name="y")    private float y;    @XmlAttribute(name="z")    private float z;    @XmlAttribute(name="h")    private byte heading;}
