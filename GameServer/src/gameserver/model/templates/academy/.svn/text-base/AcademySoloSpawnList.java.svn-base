package gameserver.model.templates.academy;import gameserver.model.PlayerClass;import gameserver.model.Race;import javax.xml.bind.annotation.XmlAccessType;import javax.xml.bind.annotation.XmlAccessorType;import javax.xml.bind.annotation.XmlAttribute;import javax.xml.bind.annotation.XmlElement;import javax.xml.bind.annotation.XmlType;import java.util.ArrayList;import java.util.List;@XmlAccessorType(value=javax.xml.bind.annotation.XmlAccessType.FIELD)@XmlType(name="academy_spawn_solo")public class AcademySoloSpawnList{    public AcademySoloSpawnList() {        pool = 0;        time = 0;        point = 0;    }    public int getMapid() {        return mapid;    }    public int getNpcid() {        return npcid;    }    public Race getRace() {        return race;    }    public PlayerClass getPlayerClass() {        return classSolo;    }    public int getPool() {        if(pool == 0)            pool = size();        return pool;    }    public int getTime() {        return time;    }    public int getPoint() {        return point;    }    public List getObjects() {        if(academyObjects == null)            academyObjects = new ArrayList();        return academyObjects;    }    public int size() {        return getObjects().size();    }    @XmlAttribute(name="map")    private int mapid;    @XmlAttribute(name="npcid")    private int npcid;    @XmlAttribute(name="pool")    private int pool;    @XmlAttribute(name="race")    private Race race;    @XmlAttribute(name="classSolo")    private PlayerClass classSolo;    @XmlAttribute(name="time")    private int time;    @XmlAttribute(name="point")    private int point;    @XmlElement(name="aloc_solo")    private List academyObjects;}
