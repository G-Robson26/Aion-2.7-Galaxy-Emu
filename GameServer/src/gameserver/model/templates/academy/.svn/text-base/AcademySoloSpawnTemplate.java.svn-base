package gameserver.model.templates.academy;import javax.xml.bind.annotation.XmlAccessType;import javax.xml.bind.annotation.XmlAccessorType;import javax.xml.bind.annotation.XmlAttribute;import javax.xml.bind.annotation.XmlTransient;import javax.xml.bind.annotation.XmlType;@XmlAccessorType(value=javax.xml.bind.annotation.XmlAccessType.FIELD)@XmlType(name="aloc_solo")public class AcademySoloSpawnTemplate{    public AcademySoloSpawnTemplate() {        spawnId = 0;    }    public int getWorldId() {        return academySoloSpawnGroup.getMapid();    }    public int getNpcId() {        return academySoloSpawnGroup.getNpcid();    }    public float getX() {        return x;    }    public float getY() {        return y;    }    public float getZ() {        return z;    }    public byte getHeading() {        return heading;    }    public int getWalkerId() {        return walkerId;    }    public AcademySoloSpawnList getSpawnGroup() {        return academySoloSpawnGroup;    }    public void setSpawnGroup(AcademySoloSpawnList academysolospawnlist) {        academySoloSpawnGroup = academysolospawnlist;    }    public void setSpawnId(int i) {        spawnId = i;    }    public int getSpawnId() {        return spawnId;    }    @XmlTransient    private AcademySoloSpawnList academySoloSpawnGroup;    @XmlAttribute(name="w")    private int walkerId;    @XmlAttribute(name="h")    private byte heading;    @XmlAttribute(name="z")    private float z;    @XmlAttribute(name="y")    private float y;    @XmlAttribute(name="x")    private float x;    private int spawnId;}
