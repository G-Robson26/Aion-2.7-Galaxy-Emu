package gameserver.model.academy;import gameserver.model.templates.academy.AcademyReSpawnTemplate;import gameserver.model.templates.academy.AcademyTeleportTemplate;public class AcademyTeleportStage{    public AcademyTeleportStage()    {    }    public AcademyTeleportTemplate getTeleportTemplate()    {        if(teleportTemplate == null)            teleportTemplate = new AcademyTeleportTemplate();        return teleportTemplate;    }    public AcademyReSpawnTemplate getRespawnTemplate()    {        if(respawnTemplate == null)            respawnTemplate = new AcademyReSpawnTemplate();        return respawnTemplate;    }    public int getStage()    {        return id;    }    protected AcademyTeleportTemplate teleportTemplate;    protected AcademyReSpawnTemplate respawnTemplate;    protected int id;}